<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="nopCommerce Test Suite" parallel="methods" thread-count="2">
    <parameter name="env" value="${env}"/>
    <parameter name="baseUrl" value="${baseUrl}"/>
    <parameter name="browser" value="${browser}"/>
    <parameter name="headless" value="${headless}"/>

    <listeners>
        <listener class-name="com.nopcommerce.bdd.listeners.TestNGListener"/>
    </listeners>

    <test name="Cucumber Tests">
        <classes>
            <class name="com.nopcommerce.bdd.runners.CucumberTestRunner"/>
        </classes>
    </test>
</suite>
